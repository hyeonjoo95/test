<!DOCTYPE html>
<html>
  <head>
    <title>TinaProject</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="header_right">
		<div class="sp">
			<%= link_to '설정' %>
			<% if model_signed_in? %>
				
				<span class="sp1"><%= link_to current_model.id ,  edit_model_registration_path%></span>
				
          		<span class="sp1"><%= link_to "Logout", destroy_model_session_path, method: :delete %></span>
        	<% else %>
          		<span class="sp1"><%= link_to "Sign up now!",  new_model_registration_path%></span>
          		<%= link_to "Log in", new_model_session_path%><br />
			<% end %>

		</div>
	</div>
    <%= yield %>

  </body>
</html>
